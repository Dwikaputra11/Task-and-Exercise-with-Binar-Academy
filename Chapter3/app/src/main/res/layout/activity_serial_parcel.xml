<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".topic2.SerialParcel">

    <TextView
        android:id="@+id/tv_nama_parcel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_age_parcel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/tv_nama_parcel"
        app:layout_constraintStart_toStartOf="@+id/tv_nama_parcel"
        app:layout_constraintTop_toBottomOf="@+id/tv_nama_parcel" />

    <TextView
        android:id="@+id/tv_city_parcel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/tv_age_parcel"
        app:layout_constraintStart_toStartOf="@+id/tv_age_parcel"
        app:layout_constraintTop_toBottomOf="@+id/tv_age_parcel" />
</androidx.constraintlayout.widget.ConstraintLayout>